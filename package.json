{
  "name": "tech-test-fractal-server",
  "version": "1.0.0",
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "node -r module-alias/register dist/src/index.js || echo 'Build not found. Run > npm run build'",
    "start:prod": "npx prisma migrate deploy && node dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint ./src",
    "orm:generate": "prisma generate",
    "orm:migrate-reset": "prisma migrate reset",
    "orm:migrate:dev": "prisma migrate dev",
    "orm:seed:dev": "ts-node prisma/seeds/seed.dev.ts",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
